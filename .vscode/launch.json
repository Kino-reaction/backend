  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug server",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run-script", "start::debug"]
    },
    {
      "name": "Debug scrapper",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run-script", "scrape"]
    },
    {
      "name": "Debug rotator",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run-script", "rotate"]
    }
  ]
}
